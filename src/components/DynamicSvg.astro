---
import Tech from './../../lib/tech.json'
import NextJs from './icons/NextJs.astro'
import React from './icons/React.astro'
import Supabase from './icons/Supabase.astro'
import Tailwind from './icons/Tailwind.astro'
import WebComponents from './icons/WebComponents.astro'
import Ts from './icons/Ts.astro'

interface Props {
    svgName: string
}

const {svgName} = Astro.props
---

{svgName && 
    (svgName === 'Web components' && <WebComponents />) || 
    (svgName === 'TypeScript' && <Ts />) ||
    (svgName === 'React' && <React />) || 
    (svgName === 'Tailwind Css' && <Tailwind />) ||
    (svgName === 'Supabase' && <Supabase />)
}
